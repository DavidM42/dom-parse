{
  "name": "dom-parse",
  "version": "2.0.2",
  "description": "Isomorphic library to safely parse an HTML string into DOM tree.",
  "main": "lib/server.cjs.js",
  "module": "lib/server.esm.js",
  "browser": {
    "./lib/server.cjs.js": "./lib/browser.cjs.js",
    "./lib/server.esm.js": "./lib/browser.esm.js"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write .",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "test": "npm-run-all -p test:*",
    "test:browser": "jest --coverage --env=jsdom src/__tests__/browser.test.js",
    "test:server": "jest --coverage --env=node src/__tests__/server.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobbuck/dom-parse.git"
  },
  "keywords": [
    "dom",
    "domparser",
    "html",
    "parse",
    "parser"
  ],
  "author": "Jacob Buck <buck.jacob@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacobbuck/dom-parse/issues"
  },
  "homepage": "https://github.com/jacobbuck/dom-parse",
  "dependencies": {
    "jsdom": "^16.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@rollup/plugin-babel": "^5.0.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rollup": "^2.18.0"
  }
}
